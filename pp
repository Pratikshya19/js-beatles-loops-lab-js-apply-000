function theBeatlesPlay(musician, instruments) {
var temp = [];
for(var i=0; i<musician.length; i++) {
temp.push(musician[i] + "plays" + instruments[i]);
}
return temp;
}

function johnLennonFacts(facts) {
const temp = [];
let i=0;
while(i<facts.length) {
temp.push(`${facts[i]} !!!`)
i++
}
return temp;
}
